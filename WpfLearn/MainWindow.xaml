    </Window.Resources>
    <Grid>
        <UserControls:ComboBoxNoItem1 x:Name="comboBoxNoItem1" HorizontalAlignment="Left" Margin="24,10,0,0" VerticalAlignment="Top" Height="44" Width="200"/>
        <UserControls:ComboBoxNoItem1 x:Name="comboBoxNoItem2" HorizontalAlignment="Left" Margin="24,59,0,0" VerticalAlignment="Top" Height="36" Width="358"/>
    </Grid>


</Window>
